{
  "name": "obsidian-quran-plugin",
  "version": "1.7.0",
  "description": "Obsidian plugin to find and insert any Quran Ayah (verse) in your notes",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "lint": "eslint main.ts src/**/*.ts tests/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier:ci": "prettier --check main.ts src/**/*.ts tests/**/*.ts .prettierrc package.json",
    "prettier:fix": "prettier --write main.ts src/**/*.ts tests/**/*.ts .prettierrc package.json",
    "normalize": "npx --yes tsx scripts/normalize_text.ts",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "prebuild": "npx tsx scripts/enrich_data.ts",
    "version": "node version-bump.mjs",
    "test": "jest",
    "enrich": "npx tsx scripts/enrich_data.ts"
  },
  "keywords": [
    "obsidian-plugin",
    "quran"
  ],
  "author": "Ammar Alakkad",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "builtin-modules": "5.0.0",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "obsidian": "^1.12.2",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  }
}
